{
  "name": "Move tabs between windows",
  "version": "1.0",
  "description": "Click the extension's toolbar button, then switch to another Chrome window â€” selected tab(s) will move to that window.",
  "manifest_version": 2,
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Move tabs to another window"
  },
  "commands": {
    "move-tabs": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Ctrl+Shift+E"
      },
      "description": "Move tabs to another window"
    }
  },
  "permissions": [
    "tabs"
  ]
}
